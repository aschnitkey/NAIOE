			+bio-mgmt("Mitchell Vallis", "Property Manager", "614-629-5208", "mvallis", "NAIOhioEquities", "NAIOhioEquities", "https://www.linkedin.com/in/mitchell-vallis-56b5a3120/", "mvallis", "customurl")
