- var titleContent = "NAI Ohio Equities - Worthington Gateway"
- var metaContent = "NAI Ohio Equities has listed several offices and retail space for lease at Worthington Gateway in Worthington, Ohio."

doctype html
html(lang="en")
	include ../../includes/head-brokers.pug
	body
		include ../../includes/nav-brokers.pug
		.navspacer
		.page-wrap.lima-selector
			h2.text-center
				small Worthington Gateway
				br
				| Office and Retail Space For Lease
			.selector-body-lima
				ul.office-agents.camden-selector-ul
					a(href="https://www.ohioequities.com/listings.html?propertyId=Worthington-Gateway", target="_blank", rel="noopener noreferrer")
						li
							img(src="https://s3.amazonaws.com/buildout-production/datas/10417225/a76742350576feceaa1d565f68feff1a8cddc058/full.jpg?1631114633", alt="7007 N High Street")
							p
								strong(style="font-size: 1.5rem") 89-105 N Wilson Bridge Rd
								br
								| Class "A" Office For Lease<br>Mixed-Use Development
					a(href="https://www.ohioequities.com/listings.html?propertyId=121-W-Wilson-Bridge-Rd", target="_blank", rel="noopener noreferrer")
						li
							img(src="https://s3.amazonaws.com/buildout-production/datas/14725475/d8fbd4af0903686a2f05352e4307ed00fb23f467/full.jpg?1678807027", alt="121 W Wilson Bridge Road")
							p 
								strong(style="font-size: 1.5rem") 121 W Wilson Bridge Road
								br
								| Trivium at Worthington Gateway<br>Office &amp; Retail For Lease
		script.
			const items = document.querySelectorAll('.camden-selector-ul li');
			items.forEach((item) => {
			item.style.boxShadow = "0 1px 3px rgba(0,0,0,0.12), 0 1px 2px rgba(0,0,0,0.24)";
			item.addEventListener('mouseover', () => {
			item.style.boxShadow = "0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22)";
			});
			item.style.transition = "all .3s ease-out"
			item.addEventListener('mouseout', () => {
			item.style.boxShadow = "0 1px 3px rgba(0,0,0,0.12), 0 1px 2px rgba(0,0,0,0.24)";
			});
			});
	include ../../includes/footer-brokers.pug
	include ../../includes/scripts.pug
