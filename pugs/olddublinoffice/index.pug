doctype html
html(lang="en")
	include ../../includes/head-brokers.pug
	body
		include ../../includes/nav-brokers.pug
		.navspacer
		.page-wrap.lima-selector
			h2.text-center
				small Old Dublin
				br
				| Office Space For Lease
			.selector-body-lima
				ul.office-agents.camden-selector-ul
					a(href="https://www.ohioequities.com/listings.html?propertyId=24-S-High-St", target="_blank")
						li
							img(src="https://s3.amazonaws.com/buildout-production/datas/3841918/96adc0e33dfbb869bb74c8c7b11348e91cf226c2/full.jpg?1572876758", alt="24 S High Street")
							p
								strong(style="font-size: 1.5rem") 24 S High Street
								br
								| 760 SF creative/loft office space<br>Open/Collaborative Work Area
					a(href="https://www.ohioequities.com/listings.html?propertyId=76-78-s-high-street", target="_blank")
						li
							img(src="https://s3.amazonaws.com/buildout-production/datas/12804347/014cc9eaa3adfc95368030fd21f5a3e266da6d42/full.jpg?1656076146", alt="76-78 S High Street")
							p 
								strong(style="font-size: 1.5rem") 76-78 S High Street
								br
								| Â±2,400 SF free standing office building<br>High-End Finishes Throughout
		script.
			const items = document.querySelectorAll('.camden-selector-ul li');
			items.forEach((item) => {
			item.style.boxShadow = "0 1px 3px rgba(0,0,0,0.12), 0 1px 2px rgba(0,0,0,0.24)";
			item.addEventListener('mouseover', () => {
			item.style.boxShadow = "0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22)";
			});
			item.style.transition = "all .3s ease-out"
			item.addEventListener('mouseout', () => {
			item.style.boxShadow = "0 1px 3px rgba(0,0,0,0.12), 0 1px 2px rgba(0,0,0,0.24)";
			});
			});
	include ../../includes/footer-brokers.pug
	include ../../includes/scripts.pug
